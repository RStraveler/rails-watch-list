<div class="container">
  <h1><%= @list.name %></h1>
  <div class="card-product">
    <% @list.movies.each do |movie| %>
    <h2><%= movie.title %></h2>
    <%= image_tag movie.poster_url %>
    <% end %>
  </div>
  <div  class="btn-link">
  <%= link_to "Add bookmark", new_list_bookmark_path(@list) %>
  </div>
</div>
